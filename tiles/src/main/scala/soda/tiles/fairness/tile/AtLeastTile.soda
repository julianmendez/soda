
class AtLeastTile

  abstract

  apply (message : TileMessage [Seq [TilePair [Measure] [Measure] ] ] )
    : TileMessage [Boolean] =
    TileMessageBuilder .mk .build (message .context) (message .outcome) (
      ( (message .contents)
        .map (lambda pair --> (pair .fst .compareTo (pair .snd) >= 0 ) )
        .forall (lambda e --> e)
      )
    )

end

