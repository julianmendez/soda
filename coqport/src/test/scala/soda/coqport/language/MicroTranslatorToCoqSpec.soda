package soda.coqport.language


* MicroTranslatorToCoqSpec()
  extends org.scalatest.funsuite.AnyFunSuite = {

  test("Coq translation of a constant")
    let
      original = "x = 0 ;"
      expected = "Definition x := 0 .\n"
      obtained = MicroTranslatorToCoq_().translate_program(original)
    in assert(obtained == expected)

  test("Coq translation of a function")
    let
      original = "f (a: nat) = 0 ;"
      expected = "Definition f (a: nat ) := 0 .\n"
      obtained = MicroTranslatorToCoq_().translate_program(original)
    in assert(obtained == expected)

}


