package soda.coqport.language


/**
 * This class translates Soda snippets into Coq snippets.
 */
* MicroTranslator = {
  + soda.lib.SomeSD_

  tc = TranslationConstant_()

  new_line = "\n"

  translate_program(program: String): String =
    SomeSD_(program)
      .value
}

* MicroTranslator_() extends MicroTranslator

