package se.umu.cs.rai.scopus.collection


* MSeq[T] {
  - isEmpty: Boolean

  - head(): T

  - tail(): MSeq[T]
}


* ESeq[T] ()
  extends MSeq[T] {

  isEmpty = true

  head(): T = throw new UnsupportedOperationException

  tail(): MSeq[T] = throw new UnsupportedOperationException

}


* NESeq[T] (head0: T, tail0: MSeq[T])
  extends MSeq[T] {

  isEmpty = false

  head() = head0

  tail() = tail0

}


