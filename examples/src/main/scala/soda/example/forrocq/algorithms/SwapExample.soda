
class PairExample

  abstract
    left : nat
    right : nat

end

class SwapExample

  abstract

  swap (pair : PairExample) : PairExample =
    PairExample_ (pair .right ) (pair .left )

  directive rocq
  Theorem
    swap_of_swap : forall (x : nat) (y : nat) , (swap (swap (PairExample_ (x) (y) ) ) ) =
    PairExample_ (x) (y) .
  Proof.
    auto.
  Qed.

end

