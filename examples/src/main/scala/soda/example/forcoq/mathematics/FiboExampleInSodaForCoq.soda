package soda.example.forcoq.mathematics

* FiboExampleInSodaForCoq = {

  + soda.example.forcoq.lib.nat
  + soda.example.forcoq.lib.O
  + soda.example.forcoq.lib.S

  _rec_fib (m: nat) (a: nat) (b: nat): nat =
    match m {
      | O() -> a
      | S (O()) -> b
      | S (k) -> _rec_fib (k) (b) (a .add (b))
    }

  fib (n: nat) =
    _rec_fib (n) (O()) ( S (O() ) )

}

* FiboExampleInSodaForCoq_()
  extends FiboExampleInSodaForCoq

