package soda.example.forcoq


class FiboExampleInSodaForCoq = {
  + soda.example.forcoq.lib.{nat, O, S}

  rec_fib (m: nat) (a: nat) (b: nat): nat =
    match m {
      | O() -> a
      | S (O()) -> b
      | S (k) -> rec_fib (k) (b) (a .add (b))
    }

  fib (n: nat) =
    rec_fib (n) (O()) ( S (O() ) )
}

class FiboExampleInSodaForCoq_()
  extends FiboExampleInSodaForCoq

