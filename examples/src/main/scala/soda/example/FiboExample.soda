package soda.example


class FiboExample is {

  has fib (n: Int): Int
}

class FiboExampleInSoda
  extends FiboExample is {

  fib (n: Int) is
    let
      result is rec (n, 0, 1)

      rec (m: Int, a: Int, b: Int): Int is
        if m == 0 then a
        else if m == 1 then b
        else rec (m - 1, b, a + b)

    in result
}

