package soda.example.algorithms

* PairExample = {

  has left: Int

  has right: Int

}

* PairExample_ (left: Int, right: Int)
  extends PairExample

* SwapExample = {

  swap (pair: PairExample): PairExample =
    PairExample_ (pair.right, pair.left)

  theorem
    swap_of_swap: forall (pair: PairExample), (swap (swap (pair) ) ) == pair

  proof
    intros p.
    destruct p.
    compute.
    apply eq_refl.

}

