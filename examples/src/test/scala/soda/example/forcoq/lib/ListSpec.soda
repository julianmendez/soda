package soda.example.forcoq.lib

class ListSpec ()
  extends
    org.scalatest.funsuite.AnyFunSuite

  test ("list from Seq")
    let
      instance = SeqList_ ()
      expected = (cons_ (0, cons_ (1, cons_ (1, cons_ (2, cons_ (3, cons_ (5, nil_ [Int] () ) ) ) ) ) ) )
      obtained =
        instance.from_Seq ( Seq[Int] (0, 1, 1, 2, 3, 5) )
    in assert (obtained == expected)

  test ("list to Seq")
    let
      instance = SeqList_ ()
      expected = Seq[Int] (1, 2, 4, 8, 16)
      obtained =
        instance.to_Seq (
          (cons_ (1, cons_ (2, cons_ (4, cons_ (8, cons_ (16, nil_ [Int] () ) ) ) ) ) )
        )
    in assert (obtained == expected)

end

