package soda.example.forcoq.lib

class ListSpec ()
  extends
    org.scalatest.funsuite.AnyFunSuite

  test ("list from Seq")
    let
      instance = SeqList_ ()
      expected = (cons (0, cons (1, cons (1, cons (2, cons (3, cons (5,  nil[Int] () ) ) ) ) ) ) )
      obtained =
        instance.from_Seq ( Seq[Int] (0, 1, 1, 2, 3, 5) )
    in assert (obtained == expected)

  test ("list to Seq")
    let
      instance = SeqList_ ()
      expected = Seq[Int] (1, 2, 4, 8, 16)
      obtained =
        instance.to_Seq (
          (cons (1, cons (2, cons (4, cons (8, cons (16,  nil[Int] () ) ) ) ) ) )
        )
    in assert (obtained == expected)

end

