
class Table

  abstract
    table : Seq [Tuple2 [String, String] ]

end

class TableTranslator
  extends
    Table
    soda.translator.block.Translator

  abstract
    table : Seq [Tuple2 [String, String] ]

  keys : Seq [String] = table.map ( lambda pair --> pair._1)

  translate : String -> String =
    lambda word -->
      table
        .toMap
        .get (word)
        .getOrElse (word)

end

