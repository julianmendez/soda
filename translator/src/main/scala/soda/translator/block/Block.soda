package soda.translator.block

* Block = {

  has lines: Seq[String]

  new_line = "\n"

  contents: String =
    lines.mkString(new_line)

}

* Block_(lines: Seq[String])
  extends Block

* BlockTranslator = {

  has source: String

  has target: String

  has translate (block: Block): Block

}

* DefaultBlockTranslator_()
  extends BlockTranslator = {

  source = "default"

  target = "default"

  translate (block: Block): Block =
    block

}

