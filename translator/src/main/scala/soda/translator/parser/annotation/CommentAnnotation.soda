package soda.translator.parser.annotation

* CommentAnnotation
  extends BlockAnnotation = {

  + soda.translator.parser.SodaConstant_

  identifier = BlockAnnotationEnum_ ().comment

  applies: Boolean =
    block
      .annotated_lines
      .forall (annotated_line -> annotated_line.is_comment)

}

* CommentAnnotation_ (block: soda.translator.block.Block)
  extends CommentAnnotation

