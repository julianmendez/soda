package soda.translator.parser.annotation

* ProofBlockAnnotation
  extends BlockAnnotation = {

  + soda.translator.block.BlockAnnotationEnum_
  + soda.translator.parser.SodaConstant_

  identifier = BlockAnnotationEnum_ ().proof_block

  applies: Boolean =
    starts_with_prefix_and_space (SodaConstant_ ().proof_reserved_word)

}

* ProofBlockAnnotation_ (block: soda.translator.block.Block)
  extends ProofBlockAnnotation

