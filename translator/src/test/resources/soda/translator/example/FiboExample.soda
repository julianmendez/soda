package soda.example

class FiboExample

  abstract
    fib (n: Int): Int

end

class FiboExampleInSoda
  extends
    FiboExample

  _rec (m: Int, a: Int, b: Int): Int is
    if m == 0 then a
    else if m == 1 then b
    else _rec (m - 1, b, a + b)

  fib (n: Int) is
    _rec (n, 0, 1)

end

class FiboExampleInSoda_ ()
  extends
    FiboExampleInSoda

end

