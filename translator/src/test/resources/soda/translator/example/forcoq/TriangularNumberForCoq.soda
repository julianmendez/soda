package soda.example.forcoq

* TriangularNumberForCoq = {

  + soda.example.forcoq.lib.nat
  + soda.example.forcoq.lib.O
  + soda.example.forcoq.lib.S

  _rec_get_number (m: nat) (acc: nat): nat =
    match m
      | O() => acc
      | S (k) => _rec_get_number (k) (acc .add ( S (k) ))
    end

  get_number (n: nat): nat =
    _rec_get_number (n) ( O() )

}

* TriangularNumberForCoq_()
  extends TriangularNumberForCoq

