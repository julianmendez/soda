package soda.example.forcoq

* FactorialForCoq = {

  import
    soda.example.forcoq.lib.nat
    soda.example.forcoq.lib.O
    soda.example.forcoq.lib.S

  _rec_get_factorial (m: nat) (product: nat): nat =
    match m
      | O() => product
      | S (k) => _rec_get_factorial (k) (product .mul ( S (k) ))
    end

  get_factorial (n: nat): nat =
    _rec_get_factorial (n) (S ( O() ) )

}

* FactorialForCoq_()
  extends FactorialForCoq

