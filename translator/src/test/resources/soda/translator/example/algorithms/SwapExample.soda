package soda.example.algorithms

class PairExample

  abstract
    left: Int
    right: Int

end

class PairExample_ (left: Int, right: Int)
  extends
    PairExample

end

class SwapExample

  left (pair: PairExample): Int =
    match pair
      | (PairExample_ (x, y) ) => x
    end

  right (pair: PairExample): Int =
    match pair
      | (PairExample_ (x, y) ) => y
    end

  swap (pair: PairExample): PairExample =
    PairExample_ (pair.right, pair.left)

  theorem
    swap_of_swap: forall (pair: PairExample), (swap (swap (pair) ) ) == pair

  proof
    intros p.
    destruct p.
    compute.
    apply eq_refl.

end

