
⎾ FizzBuzzPatternUnicode

  fizz = "Fizz"

  buzz = "Buzz"

  range = soda.lib.Range_ ()

  get_term (n : Int) : String =
    match (n % 3 , n % 5)
      case (0 , 0) ⟹ fizz + buzz
      case (0 , x) ⟹ fizz
      case (x , 0) ⟹ buzz
      case otherwise ⟹ n .toString

  apply : Seq [String] =
    range.apply (100)
      .map (λ x ⟶ x + 1)
      .map (get_term)

⎿
