package soda.example.mathematics

class FiboExample

  abstract
    fib : Int -> Int

end

class FiboExampleInSoda
  extends
    FiboExample

  _rec (m : Int) (a : Int) (b : Int) : Int =
    if m == 0 then a
    else if m == 1 then b
    else _rec (m - 1) (b) (a + b)

  fib : Int -> Int =
    any n --> fib_for (n)

  fib_for (n : Int) =
    _rec (n) (0) (1)

end
