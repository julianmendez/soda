package soda.translator.example


* FizzBuzz = {
  + soda.lib.Rec

  Fizz = "Fizz"
  Buzz = "Buzz"

  fizz_buzz() =
    Rec()
      .range(100)
      .map(x -> x + 1)
      .map(fizz_buzz_term)

  fizz_buzz_term(n: Int) =
    if n % 15 == 0 then Fizz + Buzz
    else if n % 3 == 0 then Fizz
    else if n % 5 == 0 then Buzz
    else n.toString
}

* FizzBuzzImpl() extends FizzBuzz

