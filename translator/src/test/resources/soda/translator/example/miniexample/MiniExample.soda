package soda.example.miniexample

class MiniExample

  import
    soda.lib.Range_

  _range = Range_ ()

  run () : Seq [Unit] =
    _range.apply (50)
      .map ( any x --> print (" " + (2 * x + 1) ) )

end

class Main

  main (arguments : Array [String] ) : Unit =
    MiniExample_ ().run ()

end

