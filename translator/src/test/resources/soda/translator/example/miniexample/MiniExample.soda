package soda.example.miniexample

class MiniExample
  extends
    soda.lib.Recursion

  run () =
    range (50)
      .map (x -> print (" " + (2 * x + 1)) )

end

class MiniExample_ ()
  extends
    MiniExample

end

@main

class MainClass

  main (arguments: Array [String]): Unit =
    MiniExample_ ().run ()

end

class Main ()
  extends
    MainClass

end

