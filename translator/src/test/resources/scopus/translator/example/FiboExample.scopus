package scopus.translator.example


class FiboExample {

  has fib (n in Int) in Int

}


class FiboExampleInScopus () extends FiboExample {

  fib (n in Int) is {
    result is fa (n, 0, 1)

    fa (m in Int, a in Int, b in Int) in Int is
      if m == 0 then a
      else if m == 1 then b
      else fa (m - 1, b, a + b)

    result
  }

}
