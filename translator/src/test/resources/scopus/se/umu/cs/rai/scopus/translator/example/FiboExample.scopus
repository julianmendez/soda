package se.umu.cs.rai.scopus.translator.example

import scala.annotation.tailrec


class Fibo_example {

  has fib (n: Int): Int

}


class Fibo_example_in_scopus () extends Fibo_example {

  fa (m: Int, a: Int, b: Int): Int =
    if m == 0 then a
    else if m == 1 then b
    else fa (m - 1, b, a + b)

  fib (n: Int) = fa (n, 0, 1)

}
