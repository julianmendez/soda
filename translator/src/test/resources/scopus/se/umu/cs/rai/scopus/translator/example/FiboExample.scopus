package se.umu.cs.rai.scopus.translator

import scala.annotation.tailrec


trait Fibo_example {

  fib(n: Number): Number

}


class Fibo_example_in_scopus() extends Fibo_example {

  fa(m: Number, a: Number, b: Number) =
    if m == 0 then a
    else if m == 1 then b
    else fa(m - 1, b, a + b)

  fib(n: Number) = fa(n, 0, 1)

}
